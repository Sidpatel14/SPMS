@model IEnumerable<SPMS.Models.Application>

<h2>My Applications</h2>

<p>
    <a class="btn btn-primary" asp-action="Create">New Application</a>
</p>

<table class="table">
    <thead>
        <tr>
            <th>Permit Type</th>
            <th>Status</th>
            <th>Submitted Date</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var app in Model)
        {
            <tr>
                <td>@app.PermitType</td>
                <td>@app.Status</td>
                <td>@app.SubmissionDate</td>
            </tr>
        }
    </tbody>
</table>

